msgid "[[!meta title=\"Developer Toolkit\"]]\n"
msgstr "[[!meta title=\"Инструментариум за разработчици (Developer Toolkit)\"]]\n"

#. type: Bullet: '* '
msgid "Author: Andy Borka"
msgstr "Автор: Andy Borka"

#. type: Bullet: '* '
msgid "download [stable version][1]"
msgstr "Изтегляне на [стабилна версия][1]"

#. type: Bullet: '* '
