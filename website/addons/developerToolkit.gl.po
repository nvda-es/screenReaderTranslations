msgstr "[[!meta title=\"Developer Toolkit\"]]\n"

#. type: Bullet: '* '
msgid "Author: Andy Borka"
msgstr "Autor: Andy Borka"

#. type: Bullet: '* '
msgid "download [stable version][1]"
msgstr "descargar [versión estable][1]"

#. type: Bullet: '* '
