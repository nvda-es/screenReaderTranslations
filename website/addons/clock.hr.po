#, no-wrap
msgid "Key commands"
msgstr "Tipkovnički prečaci"

#. type: Bullet: '* '
msgid "NVDA+F12, get current time;"
msgstr ""

#. type: Bullet: '* '
msgid "NVDA+F12 pressed twice quickly, get current date;"
msgstr ""

#. type: Bullet: '* '
#, fuzzy
#| msgid ""
