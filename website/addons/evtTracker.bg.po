msgid "[[!meta title=\"Event Tracker\"]]\n"
msgstr "[[!meta title=\"Проследяване на събития (Event Tracker)\"]]\n"

#. type: Bullet: '* '
#, fuzzy
#| msgid "Author: Joseph Lee"
msgid "Author: Joseph Lee, Thiago Seus"
msgstr "Автор: Joseph Lee"

#. type: Bullet: '* '
