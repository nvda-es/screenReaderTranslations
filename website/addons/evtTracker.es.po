msgid "[[!meta title=\"Event Tracker\"]]\n"
msgstr "[[!meta title=\"Rastreador de eventos\"]]\n"

#. type: Bullet: '* '
#, fuzzy
#| msgid "Author: Joseph Lee"
msgid "Author: Joseph Lee, Thiago Seus"
