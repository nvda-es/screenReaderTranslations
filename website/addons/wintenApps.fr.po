msgid "[[!meta title=\"Windows App Essentials\"]]\n"
msgstr "[[!meta title=\"Windows App Essentials\"]]\n"

#. type: Bullet: '* '
msgid "Authors: Joseph Lee, Derek Riemer and others"
msgstr "Auteurs: Joseph Lee, Derek Riemer et autres utilisateurs"

#. type: Bullet: '* '
msgid "Download [stable version][1]"
msgstr "Télécharger [version stable][1]"

#. type: Bullet: '* '
msgid "Download [beta version][2]"
msgstr "Télécharger [version béta][2]"

#. type: Bullet: '* '
msgid "Download [development version][3]"
msgstr "Télécharger [version de développement][3]"

#. type: Bullet: '* '
