msgstr "[[!meta title=\"Copiador do cursor de exploração\"]]\n"

#. type: Bullet: '* '
msgid "Author: Tuukka Ojala <tuukka.ojala@gmail.com>"
msgstr "Autor: Tuukka Ojala <tuukka.ojala@gmail.com>"

#. type: Bullet: '* '
msgid "Download [stable version][1]"
msgstr "Baixe a [versão estável][1]"

#. type: Bullet: '* '
