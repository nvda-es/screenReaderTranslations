#, no-wrap
msgid ""
"    <math xml:lang=\"en\">\n"
"      <semantics>\n"
"        <mrow class=\"MJX-TeXAtom-ORD\">\n"
"          <mstyle displaystyle=\"true\" scriptlevel=\"0\">\n"
"            <mo stretchy=\"false\">(</mo>\n"
"            <mn>1</mn>\n"
"            <mo>+</mo>\n"
"            <mi>x</mi>\n"
"            <msup>\n"
"              <mo stretchy=\"false\">)</mo>\n"
"              <mrow class=\"MJX-TeXAtom-ORD\">\n"
"                <mi>α<!-- α --></mi>\n"
"              </mrow>\n"
"            </msup>\n"
"            <mo>=</mo>\n"
"            <munderover>\n"
"              <mo>∑<!-- ∑ --></mo>\n"
"              <mrow class=\"MJX-TeXAtom-ORD\">\n"
"                <mi>n</mi>\n"
"                <mo>=</mo>\n"
"                <mn>0</mn>\n"
"              </mrow>\n"
"              <mrow class=\"MJX-TeXAtom-ORD\">\n"
"                <mi mathvariant=\"normal\">∞<!-- ∞ --></mi>\n"
"              </mrow>\n"
"            </munderover>\n"
"            <mi>C</mi>\n"
"            <mo stretchy=\"false\">(</mo>\n"
"            <mi>α<!-- α --></mi>\n"
"            <mo>,</mo>\n"
"            <mi>n</mi>\n"
"            <mo stretchy=\"false\">)</mo>\n"
"            <msup>\n"
"              <mi>x</mi>\n"
"              <mrow class=\"MJX-TeXAtom-ORD\">\n"
"                <mi>n</mi>\n"
"              </mrow>\n"
"            </msup>\n"
"            <mspace width=\"1em\"></mspace>\n"
"            <mi mathvariant=\"normal\">∀<!-- ∀ --></mi>\n"
"            <mi>x</mi>\n"
"            <mo>:</mo>\n"
"            <mrow>\n"
"              <mo>|</mo>\n"
"              <mi>x</mi>\n"
"              <mo>|</mo>\n"
"            </mrow>\n"
"            <mo>&lt;</mo>\n"
"            <mn>1</mn>\n"
"            <mo>,</mo>\n"
"            <mi mathvariant=\"normal\">∀<!-- ∀ --></mi>\n"
"            <mi>α<!-- α --></mi>\n"
