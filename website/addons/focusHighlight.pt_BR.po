msgid "[[!meta title=\"Focus Highlight\"]]\n"
msgstr "[[!meta title=\"Realse de foco\"]]\n"

#. type: Bullet: '* '
msgid "Authors: Takuya Nishimoto"
msgstr "Autores: Takuya Nishimoto"

#. type: Bullet: '* '
msgid "Download [stable version][2]"
msgstr "Baixe a [versão estável][2]"

#. type: Bullet: '* '
