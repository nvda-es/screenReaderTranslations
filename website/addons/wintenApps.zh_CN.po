msgid "[[!meta title=\"Windows App Essentials\"]]\n"
msgstr "[[!meta title=\"Windows 应用增强\"]]\n"

#. type: Bullet: '* '
msgid "Authors: Joseph Lee, Derek Riemer and others"
msgstr "作者：Joseph Lee、Derek Riemer 等"

#. type: Bullet: '* '
msgid "Download [stable version][1]"
msgstr "下载[稳定版][1]"

#. type: Bullet: '* '
msgid "Download [beta version][2]"
msgstr "下载[测试版][2]"

#. type: Bullet: '* '
msgid "Download [development version][3]"
msgstr "下载[开发板][3]"

#. type: Bullet: '* '
#, fuzzy
