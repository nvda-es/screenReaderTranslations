#: globalPlugins/resourceMonitor/__init__.py:34
msgid "No authentication (Open)"
msgstr "Kimlik doğrulama yok (Açık)"

#: globalPlugins/resourceMonitor/__init__.py:47
msgid "Connected to {}"
msgstr "{} Bağlı"

#: globalPlugins/resourceMonitor/__init__.py:50
msgid "Disconnected from {}"
msgstr "{} İle bağlantı kesildi"

#: globalPlugins/resourceMonitor/__init__.py:52
msgid "A wireless device has been enabled"
msgstr "Bir kablosuz cihaz etkinleştirildi"

#: globalPlugins/resourceMonitor/__init__.py:54
msgid "A wireless device has been disabled"
msgstr "Bir kablosuz cihaz devre dışı bırakıldı"

#: globalPlugins/resourceMonitor/__init__.py:159
msgid "This computer does not have a battery connected."
msgstr "Bu bilgisayara pil takılı değil."

#: globalPlugins/resourceMonitor/__init__.py:165
#, python-brace-format
msgid "{percent}%, battery charging."
msgstr "%{percent}, pil şarj oluyor."

#: globalPlugins/resourceMonitor/__init__.py:171
#, python-brace-format
msgid "{percent}% battery remaining, battery time unknown."
msgstr "%{percent} kalan pil, pil süresi bilinmiyor."

#: globalPlugins/resourceMonitor/__init__.py:183
#: globalPlugins/resourceMonitor/__init__.py:449
msgid "1 hour"
msgstr "1 saat"

#: globalPlugins/resourceMonitor/__init__.py:185
#: globalPlugins/resourceMonitor/__init__.py:451
#, python-brace-format
msgid "{0} hours"
msgstr "{0} saat"

#: globalPlugins/resourceMonitor/__init__.py:190
#: globalPlugins/resourceMonitor/__init__.py:455
msgid "1 minute"
msgstr "1 dakika"

#: globalPlugins/resourceMonitor/__init__.py:192
#: globalPlugins/resourceMonitor/__init__.py:457
#, python-brace-format
msgid "{0} minutes"
msgstr "{0} dakika"

#: globalPlugins/resourceMonitor/__init__.py:200
#, python-brace-format
msgid "{percent}% battery remaining, about {time}."
msgstr "%{percent} kalan pil, yaklaşık {time}."

#: globalPlugins/resourceMonitor/__init__.py:204
msgid " Warning: low battery."
msgstr " Uyarı: düşük pil."

#: globalPlugins/resourceMonitor/__init__.py:207
msgid " Warning: critically low battery."
msgstr " Uyarı: pil kritik seviyede."

#: globalPlugins/resourceMonitor/__init__.py:263
#, python-brace-format
msgid "{winVersion} ({cpuBit})"
msgstr "{winVersion} ({cpuBit})"

#: globalPlugins/resourceMonitor/__init__.py:272
msgid "Resource Monitor"
msgstr "Kaynak İzleme"

#: globalPlugins/resourceMonitor/__init__.py:284
msgid ""
"Presents battery percentage, charging status, remaining time (if not "
"charging), and a warning if the battery is low or critical."
msgstr ""
"Pil yüzdesini, şarj durumunu, kalan zamanı  (şarja takılı değilse ), ve "
"düşük ya da kritik pil seviyesini söyler."

#: globalPlugins/resourceMonitor/__init__.py:298
msgid ""
"Presents the used and total space of the static and removable drives on this "
"computer."
msgstr ""
"Bilgisayara takılı sabit ve çıkarılabilir disklerle ilgili kullanım "
"bilgisini verir."

#: globalPlugins/resourceMonitor/__init__.py:315
#, python-brace-format
msgid ""
"{driveName} ({driveType} drive): {usedSpace} of {totalSpace} used {percent}%."
msgstr ""
"{driveName} ({driveType} sürücü): Kullanılan, {usedSpace}, Toplam, "
"{totalSpace}, %{percent}."

#: globalPlugins/resourceMonitor/__init__.py:330
msgid "Presents the average processor load and the load of each core."
msgstr "Ortalama işlemci yükü ve çekirdek kullanımı bilgisini verir."

#: globalPlugins/resourceMonitor/__init__.py:339
#, python-brace-format
msgid "Core {coreNumber}: {corePercent}%"
msgstr "Çekirdek {coreNumber}: yüzde {corePercent}"

#: globalPlugins/resourceMonitor/__init__.py:346
#, python-brace-format
msgid "Average CPU load {avgLoad}%."
msgstr "Ortalama CPU yükü %{avgLoad}."

#: globalPlugins/resourceMonitor/__init__.py:349
#, python-brace-format
msgid "Average CPU load {avgLoad}%, {cores}."
msgstr "Ortalama CPU yükü %{avgLoad}, {cores}."

#: globalPlugins/resourceMonitor/__init__.py:359
msgid "Presents the used and total space for both physical and virtual ram."
msgstr "Kullanılan fiziksel ve sanal ram bilgisini verir."

#: globalPlugins/resourceMonitor/__init__.py:365
#, python-brace-format
msgid "Physical: {physicalUsed} of {physicalTotal} used ({physicalPercent}%). "
msgstr ""
"Fiziksel: {physicalUsed} bölü {physicalTotal} kullanılıyor "
"(%{physicalPercent}). "

#: globalPlugins/resourceMonitor/__init__.py:377
#, python-brace-format
msgid "Virtual: {virtualUsed} of {virtualTotal} used ({virtualPercent}%)."
msgstr ""
"Sanal: {virtualUsed} bölü {virtualTotal} kullanılıyor (%{virtualPercent})."

#: globalPlugins/resourceMonitor/__init__.py:389
msgid "Announces the version of Windows you are using."
msgstr "Kullandığınız Windows sürümünü söyler."

#: globalPlugins/resourceMonitor/__init__.py:402
msgid "Announces the system's wireless network ssid name, and its strength."
msgstr "Sistemin kablosuz ağ ssid adını ve gücünü duyurur."

#: globalPlugins/resourceMonitor/__init__.py:410
msgid "No wireless devices"
msgstr "Kablosuz cihaz yok"

#: globalPlugins/resourceMonitor/__init__.py:416
msgid "No wireless connections"
msgstr "Kablosuz bağlantı yok"

#: globalPlugins/resourceMonitor/__init__.py:423
msgid "Connected wireless network: {}. Signal strength: {}%. Security type: {}"
msgstr "Bağlı kablosuz ağ: {}. Sinyal gücü: %{}. Güvenlik türü: {}"

#: globalPlugins/resourceMonitor/__init__.py:442
msgid "1 day"
msgstr "1 gün"

#: globalPlugins/resourceMonitor/__init__.py:444
#, python-brace-format
msgid "{0} days"
msgstr "{0} gün"

#: globalPlugins/resourceMonitor/__init__.py:462
msgid "1 second"
msgstr "1 saniye"

#: globalPlugins/resourceMonitor/__init__.py:464
#, python-brace-format
msgid "{0} seconds"
msgstr "{0} saniye"

#: globalPlugins/resourceMonitor/__init__.py:470
msgid "Announces the system's uptime."
msgstr "Sistemin çalışma süresini seslendirir."

#: globalPlugins/resourceMonitor/__init__.py:482
msgid "Failed to get the system's uptime."
msgstr "Sistemin açık olduğu zaman bilgisi alınamadı."

#: globalPlugins/resourceMonitor/__init__.py:486
msgid ""
"Presents used ram, average processor load, and battery info if available."
msgstr "Kullanılan ram, CPU yükü, ve mevcutsa pil bilgisini verir ."

#: globalPlugins/resourceMonitor/__init__.py:493
#, python-brace-format
msgid "{ramPercent}% RAM used, CPU at {cpuPercent}%."
msgstr "yüzde {ramPercent} RAM kullanılan, CPU:  %{cpuPercent}."

#: installTasks.py:22
msgid "Unsupported Windows release"
msgstr "Desteklenmeyen Windows sürümü"

#: installTasks.py:30
#, python-brace-format
msgid ""
"You are using {releaseName} ({build}), a Windows release not supported by "
"this add-on.\n"
"This add-on requires {supportedReleaseName} ({supportedBuild}) or later."
msgstr ""
"You are using {releaseName} ({build}), a Windows release not supported by "
"this add-on.\n"
"Bu eklenti, {supportedReleaseName} ({supportedBuild}) veya sonraki bir "
"sürümünü gerektirir."

