#, python-format
msgid "%d hour, "
msgstr "%d hodina, "

#, python-format
msgid "%d hours, "
msgstr "%d hodin, "

#, python-format
msgid "%d minute, "
msgstr "%d minuta, "

#, python-format
msgid "%d minutes, "
msgstr "%d minut, "

#, python-format
msgid "%s second"
msgstr "%s sekunda"

#, python-format
msgid "%s seconds"
msgstr "%s sekund"

msgid "on"
msgstr "zapnuto"

msgid "off"
msgstr "vypnuto"

msgid "mute"
msgstr "stlumit"

msgid "Mute playback"
msgstr "Stlumit přehrávání"

msgid "Set playback volume to 25%"
msgstr "Nastavit hlasitost přehrávání na 25%"

msgid "Set playback volume to 50%"
msgstr "Nastavit hlasitost přehrávání na 50%"

msgid "Set playback volume to 100%"
msgstr "Nastavit hlasitost přehrávání na 100 %"

msgid "Pan left"
msgstr "Posunout doleva"

msgid "Pan right"
msgstr "Posunout doprava"

msgid "center"
msgstr "Střed"

msgid "Pan center"
msgstr "Vyvážit na střed"

msgid "No time information."
msgstr "Žádná informace o čase."

msgid "Speaks total track length"
msgstr "Přečte celkovou délku stopy."

msgid "Speaks track elapsed time"
msgstr "Přečte uplynulý čas aktuálně přehrávané stopy."

msgid "Speaks track remaining time"
msgstr "přečte zbývající čas aktuálně přehrávané stopy."

msgid "not playing"
msgstr "nepřehrává se"

msgid "Review the end of track (last 6 seconds by default)"
msgstr "Prolínání konce stopy (standardně posledných 6 sekund)."

msgid "Seconds:"
msgstr "Sekund:"

msgid "Set review time"
msgstr "nastavit čas prolínání"

msgid "Bad value entered! Please try again."
msgstr "Neplatná hodnota! Prosím zkuste to znovu."

msgid "Error"
msgstr "Chyba"

msgid ""
"Set the review time (in seconds) for use with Review End of Track command"
msgstr "Zobrazí dialog s nastavením času prolínání v sekundách."

msgid "Alternate jump forward (6 seconds by default)"
msgstr "Skok vpřed (standardně 6 sekund)."

msgid "Alternate jump backward (6 seconds by default)"
msgstr "Skok zpět (standardně 6 sekund)."

msgid "Set alternate jump time"
msgstr "Nastavit čas skoku"

msgid "Set alternate jump time (in seconds)"
msgstr "Zobrazí dialog s nastavením času skoku v sekundách."

